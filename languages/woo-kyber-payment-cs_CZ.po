msgid "Awaiting cheque payment"
msgstr "Probíhá platba šekem"

#: includes/class-woo-kyber-payment-gateway.php:403
msgid "Pay by tokens"
msgstr "Zaplatit tokeny"

#: includes/class-woo-kyber-payment-gateway.php:435
msgid "Total"
msgstr "Celkem"

#: includes/class-woo-kyber-payment.php:228
